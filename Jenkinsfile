pipeline
{  
  agent any
  stages{
    stage('Init')
    {
      steps{
         echo 'Hi init'
      }
    }
    stage('build')
    {
      steps{
        echo 'building'
      }
    }
    stage('deploy')
    {
      steps{
        echo 'deploy'
      }
    }
    stage('deploy-prod')
    {
      steps{
        echo 'prod'
      }
    }
  }
}
